{
  "name": "aura-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@nestjs/testing": "^7.0.5",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.10",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/experimental-utils": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^2.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.7",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10",
    "@nestjs/common": "^7.0.3",
    "@nestjs/core": "^7.0.3",
    "@nestjs/platform-express": "^7.0.3",
    "@nestjs/swagger": "^4.4.0",
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.4",
    "styled-components": "^5.0.1",
    "swagger-ui-express": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"yarn dev:server\"  \"yarn dev:client\"",
    "dev:server": "TS_NODE_PROJECT=server.tsconfig.json PORT=8309 nodemon --inspect=5309 server/index.ts",
    "dev:client": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --runInBand --watch",
    "test:integration": "jest --runInBand -t=integration",
    "test:all": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts --ext .tsx --fix"
  },
  "proxy": "http://localhost:8309",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "server.tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(test).(ts|tsx)"
    ],
    "roots": [
      "<rootDir>/server",
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "server/**/*.ts",
      "src/redux/**/*.ts",
      "src/components/**/*.tsx",
      "src/components/**/*.ts",
      "src/selectors/**/*.ts",
      "src/utils/**/*.ts",
      "src/domain/**/*.ts"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "yarn lint && git add && lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
